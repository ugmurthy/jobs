{
  "name": "runner",
  "version": "0.0.2",
  "description": "backend module to run jobs",
  "main": "index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "clearlogs": "rm logs/*",
    "redis-start": "exec brew services start redis",
    "redis-info": "exec brew services info redis",
    "redis-stop": "exec brew services stop redis",
    "generate-docs": "node scripts/generate-docs.js",
    "prebuild": "pnpm run generate-docs",
    "build": "tsc",
    "generate-types": "openapi-typescript src/swagger.json --output src/types/api.ts",
    "postgenerate-docs": "pnpm run generate-types",
    "dev": "pnpm run build && pnpm start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:flow": "vitest run src/tests/flow.test.ts"
  },
  "dependencies": {
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
    "@prisma/client": "^6.10.1",
    "@ugm/logger": "^1.0.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.54.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "got": "^14.4.7",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^18.19.112",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/ui": "^3.2.4",
    "openapi-typescript": "^7.8.0",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vitest": "^3.2.4"
  }
}
